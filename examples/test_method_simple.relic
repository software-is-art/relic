// Simple method dispatch test

// Define simple value types
value Meter(value: Int) {
    validate: value >= 0
}

value Foot(value: Int) {
    validate: value >= 0
}

// Methods with same name but different parameter types
method toInches(m: Meter) -> Int {
    m.value * 39
}

method toInches(f: Foot) -> Int {
    f.value * 12
}

// Test generic dispatch
method show(x: Any) -> String {
    "Something"
}

method show(n: Int) -> String {
    "Number"
}